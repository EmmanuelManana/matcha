 <div class="container bg5">
 <img src="uploads{{pp}}" width="300px" class="rounded" height="300px">
</div>
<button class = "btn btn-success"><i class="fas fa-check"></i>{{online}} </button>
<div class="container" style="margin-top:330px; margin-left: 0px">

{{#if notable}}
Upload a profile so you can like
{{/if}}

  {{#if able}}
     <form action = "/search" method = "POST" style = "display:inline">
    <input type = "hidden" value = "{{Profile}}" name = "name">
      <input type = "hidden" value = "{{pp}}" name = "profilepic">
      <input type = "hidden" value = "{{Profile}}" id = "notif">
    <input type = "hidden" value = "Notifs : " id = "Notifs">
    <input type = "hidden" id = "notifylike" value = "{{Profile}}">
    <button id = "messag" type = "submit" class = "btn btn-danger" name = "submit" value = "submit"> <i class="fas fa-heart"></i> </button>
    </form>
    {{/if}}
    {{#if fullconnec}}
    <form action = "/search" style = "display:inline" method = "POST">
    <input type = "hidden" name = "user" value = "{{Profile}}">
    <input type = "hidden" name = "unlike" value = "unlike">
    <button type = "submit" class="btn btn-danger" ><i class="fas fa-unlink"></i></button>
    </form>
    {{/if}}
  {{#if uhalfconnec}}
<button data-toggle = "tooltip" class = "btn btn-warning" title = "You have liked this user!"><i class="fas fa-star-half"></i></button>
{{/if}}
{{#if halfconnec}}
<button data-toggle = "tooltip" class = "btn btn-warning" title = "This user has liked your profile!"><i class="fas fa-star-half"></i></button>
{{/if}}
{{#if fullconnec}}
<button data-toggle = "tooltip2" class = "btn btn-warning" title = "You are now connected"> Connected <i class="fas fa-star"></i></i></button>
{{/if}}
   <button class="btn btn-info" > <i class="fas fa-fire"></i> {{fame}}</button>
      {{#if fullconnec}}
   <form action = "/chatr" method= "POST" style = "display:inline">
   <input type = "hidden" name = "user" value = {{Profile}}>
 <button class="btn btn-success" > <i class="fa fa-comments" aria-hidden="true"></i>Chat</button>
       </form>
       {{/if}}
  <form action = "/block" method = "POST" style = "display:inline">
  <input type = "hidden" name = "user" value = "{{Profile}}">
  <input type = "hidden" name = "path" value = "{{pp}}">
  <input type = "hidden" name = "block" value = "block">
      <button type = "submit" name = "block" class="btn btn-danger" >Block</button>
  </form>
  <form action = "/report" method = "POST" style = "display:inline">
  <input type = "hidden" name = "name" value = "{{Profile}}">
  <input type = "hidden" name = "report" value = "report">
    <button class="btn btn-danger" >Report</button>
  </form>
   </div> 
   <br>
